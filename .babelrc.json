{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions"]
        }
      }
    ],
    "@babel/react",
    "@babel/flow"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/syntax-object-rest-spread"
  ]
}
